<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/scrollViewPartners"
    android:layout_width="409dp"
    android:layout_height="729dp"
    android:background="@color/white"
    android:fillViewport="true"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="1dp">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:id="@+id/announcementsPage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        android:visibility="visible"
        tools:context=".Announcements">


        <TextView
            android:id="@+id/announcementsHeader"
            android:layout_width="236dp"
            android:layout_height="55dp"
            android:layout_marginTop="52dp"
            android:background="@color/white"
            android:text="Announcements"
            android:textAlignment="center"
            android:textColor="#043670"
            android:textSize="30sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="115dp"
            android:layout_height="115dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.054"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/huda_logo" />

        <TextView
            android:id="@+id/announcementsHeader2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="72dp"
            android:text="2022 Holiday Closure Dates"
            android:textAlignment="center"
            android:textColor="#043670"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/announcementsHeader" />

        <TextView
            android:id="@+id/announcementsHeader3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40sp"
            android:text="Clinic Calendar"
            android:textAlignment="center"
            android:textColor="#043670"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/date3" />

        <TextView
            android:id="@+id/date1"
            android:layout_width="230dp"
            android:layout_height="56dp"
            android:layout_marginTop="40dp"
            android:text="Wednesday November 23, 2022 - Monday November 28th , 2022"
            android:textColor="#043670"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.089"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/announcementsHeader2" />

        <TextView
            android:id="@+id/date2"
            android:layout_width="227dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:text="Saturday December 24th, 2022"
            android:textColor="#043670"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/date1" />

        <TextView
            android:id="@+id/date3"
            android:layout_width="227dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="20dp"
            android:text="Saturday December 31st, 2022"
            android:textColor="#043670"
            android:textSize="15sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/date2" />

        <TextView
            android:id="@+id/holiday1"
            android:layout_width="147dp"
            android:layout_height="29dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="52dp"
            android:text="Thanksgiving Break"
            android:textColor="#043670"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/date1"
            app:layout_constraintTop_toBottomOf="@+id/announcementsHeader2" />

        <TextView
            android:id="@+id/holiday2"
            android:layout_width="147dp"
            android:layout_height="29dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="36dp"
            android:text="Christmas Eve"
            android:textColor="#043670"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.333"
            app:layout_constraintStart_toEndOf="@+id/date2"
            app:layout_constraintTop_toBottomOf="@+id/holiday1" />

        <TextView
            android:id="@+id/holiday3"
            android:layout_width="147dp"
            android:layout_height="29dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="21dp"
            android:text="New Year's Eve"
            android:textColor="#043670"
            android:textSize="15sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/date3"
            app:layout_constraintTop_toBottomOf="@+id/holiday2" />

        <WebView
            android:id="@+id/calendarWebView"
            android:layout_width="407dp"
            android:layout_height="500dp"
            android:layout_marginTop="36dp"
            android:layout_marginEnd="2dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/announcementsHeader3" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
